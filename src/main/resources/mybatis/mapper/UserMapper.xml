<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.springboot.schedule.demo.dao.UserDao">
    <insert id="insertOne" >
        INSERT INTO USER (name,gender) VALUES (#{name},#{gender})
    </insert>
    <select id="selectAll" resultType="com.springboot.schedule.demo.entity.User">
      SELECT * FROM USER
    </select>
</mapper>